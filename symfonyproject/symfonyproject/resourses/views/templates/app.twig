<!DOCTYPE html>
<html>
  <head>

      <!-- Start meta -->
      <meta charset="utf-8">
      <meta name="description" content="dashboard">
      <meta http-equiv="X-UA-compatible" content="IE=edge">
      <meta name="keywords" content="chart-dashboard">
      <meta name="author" content="Dejavu">
      <!-- Start title -->
      <title>{% block title %}{% endblock %}</title>
      <link rel="icon" href="../images/logo.png" type="image/x-icon"/>
      <link href="https://fonts.googleapis.com/css?family=Maven+Pro" rel="stylesheet">

      <link rel="stylesheet" href="{{ base_url() }}/css/fa-svg-with-js.css">
      <link rel="stylesheet" href="{{ base_url() }}/css/fontawesome-all.min.css">
      <link rel="stylesheet" href="{{ base_url() }}/css/bootstrap.min.css">
      <link rel="stylesheet" href="{{ base_url() }}/css/dataTables.min.css">

      <link rel="icon" href="{{ base_url() }}/images/logo.png" type="image/x-icon"/>
      <link rel="shortcut icon" type="image/png" href="{{ base_url() }}/images/logo.png"/>
      <link rel="shortcut icon" type="image/png" href="{{ base_url() }}/images/logo.png"/>

      <link rel="stylesheet" href="{{ base_url() }}/css/main.css">
  </head>
  <body>
    {% include 'templates/partials/header.twig' %}
    <section class="dashboard">
        <!-- Start Dashboard Content -->
        <div class="content container-fluid">
            <div class="heading">
                <h4>
                    <span class="icon"><i class="fas fa-th-large"></i></span>
                    {% block page_title %}{% endblock %}
                </h4>
            </div>
            {% block content %}{% endblock %}
        </div>
    </section>
    </div>
    {% include 'templates/partials/footer.twig' %}
  </body>
</html>
